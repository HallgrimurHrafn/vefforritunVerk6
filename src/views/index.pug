extends layout

block content
  if channels != 'null'
    div.fixed
      if channels != null
        each item in channels
          each channel in item.channels
            form(method='post', action='/')
              input.link.col-1(name='submit', value=channel.name, type='submit')
              input(name='data', type='hidden', value=channel.endpoint)

